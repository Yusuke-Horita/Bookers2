<%= render "books/list", user: @user %>

<h1>Users</h1>
<table>
	<tr>
		<th>image</th>
		<th>name</th>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= attachment_image_tag user, :profile_image, :fill, 60, 60 %></td>
		<td><%= user.name %></td>
		<td><%= link_to "Show", user_path(user.id) %></td>
	</tr>
	<% end %>
</table>